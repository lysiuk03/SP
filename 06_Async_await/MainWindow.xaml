<Window x:Class="_06_Async_await.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_06_Async_await"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button Click="Button_Click">Generate</Button>
        </ToolBar>
        <ProgressBar IsIndeterminate="True" DockPanel.Dock="Bottom" Height="20" Value="10"></ProgressBar>
        <ListBox x:Name="list"></ListBox>
    </DockPanel>
</Window>
